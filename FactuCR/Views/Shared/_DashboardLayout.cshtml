                            <span><img id="brandName" src="~/images/FCR_Name_TR.png" style="width: 140px; margin-top: 10px; height: 40px; padding: 0;" /></span>
                        </a>
                    </div>

                    <div class="clearfix"></div>


                    <!-- /menu profile quick info -->

                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">

                        <!-- menu section for mantenimiento -->
                        <div id="mantenimiento-menu-container" class="menu_section" style="display: none;">
                            <h3>Mantenimiento</h3>
                            <ul id="sales-menu" class="nav side-menu">
                                <li>
                                    <a><i class="fa fa-user"></i> Socios <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a asp-area="" asp-controller="Clients" asp-action="Create">Agregar Cliente</a></li>
                                        <li><a asp-area="" asp-controller="Clients" asp-action="Index">Lista de Clientes</a></li>
                                        <li><a asp-area="" asp-controller="Providers" asp-action="Create">Agregar Proveedor</a></li>
                                        <li><a asp-area="" asp-controller="Providers" asp-action="Index">Lista de Proveedores</a></li>

                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-product-hunt"></i> Productos <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a asp-area="" asp-controller="Products" asp-action="Create">Agregar Producto</a></li>
                                        <li><a asp-area="" asp-controller="Products" asp-action="Index">Lista de Productos</a></li>
                                        <li><a asp-area="" asp-controller="Categories" asp-action="Create">Agregar Categor&iacute;a</a></li>
                                        <li><a asp-area="" asp-controller="Categories" asp-action="Index">Lista de Categor&iacute;as</a></li>
                                        <li><a asp-area="" asp-controller="Families" asp-action="Create">Agregar Familia</a></li>
                                        <li><a asp-area="" asp-controller="Families" asp-action="Index">Lista de Familias</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-gift"></i> Descuentos <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a asp-area="" asp-controller="Discounts" asp-action="Create">Agregar Descuento</a></li>
                                        <li><a asp-area="" asp-controller="Discounts" asp-action="Index">Lista de Descuentos</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-cubes"></i> Empresa <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a asp-area="" asp-controller="BranchOffices" asp-action="Create">Agregar Oficina</a></li>
                                        <li><a asp-area="" asp-controller="BranchOffices" asp-action="Index">Lista de Oficinas</a></li>
                                        <li><a asp-area="" asp-controller="Terminals" asp-action="Create">Agregar Terminal</a></li>
                                        <li><a asp-area="" asp-controller="Terminals" asp-action="Index">Lista de Terminales</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <!-- /menu section for mantenimiento  -->
                        <!-- menu section for billing -->
                        <div id="facturacion-menu-container" class="menu_section" style="display: none;">
                            <h3>Facturaci&oacute;n</h3>
                            <ul id="sales-menu" class="nav side-menu">
                                <li>
                                    <a><i class="fa fa-file-text-o"></i> Facturas / Tiquetes <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a asp-area="" asp-controller="Billing" asp-action="CreateBill">Crear Factura o Tiquete Electr&oacute;nico</a></li>
                                        <li><a asp-area="" asp-controller="Billing" asp-action="Index">Lista de Facturas y Tiquetes Electr&oacute;nicos</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-file-text-o"></i>Cr&eacute;dito / D&eacute;bito <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a asp-area="" asp-controller="" asp-action="CreateNote">Crear Nota de Cr&eacute;dito o D&eacute;bito</a></li>
                                        <li><a asp-area="" asp-controller="" asp-action="">Lista de Notas de Cr&eacute;dito y D&eacute;bito</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <!-- /menu section for facturacion -->

                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Settings" asp-controller="ConfigCompanies" asp-action="Index">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout" asp-controller="Users" asp-action="Logout">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="">

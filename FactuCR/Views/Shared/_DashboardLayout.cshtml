    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" href="@Url.Content("~/images/FCR_Icon_TR.ico")" />
    <link rel="icon" href="@Url.Content("~/images/FCR_Icon_TR.ico")" type="image/x-icon" />
    <link rel="icon" href="@Url.Content("~/images/FCR_Icon_TR.ico")" type="image/vnd.microsoft.icon" />

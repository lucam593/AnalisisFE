                                <a><i class="fa fa-clipboard"></i> Mantenimiento</a>
                            </li>

                            <li id="btn-facturacion" class="">
                                <a><i class="fa fa-credit-card"></i> Facturaci&oacute;n</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" style="height: 100%; display: block;" role="main">
                <!-- here goes the pages content -->
                <div style="height: 100%; display: block;">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <!-- Init Panel Title -->
                                <div class="x_title">
                                    <h2>@ViewBag.PanelTitle<small>@ViewBag.Annotation</small></h2>

                                    <div class="clearfix"></div>
                                </div>
                                <!-- Finish Panel Title -->
                                <!-- Init Panel Content -->
                                <div class="x_content">
                                    @RenderBody()
                                </div>
                                <!-- Finish Panel Content -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer id="dashboard_footer">
                <div style="text-align: center;">
                    © 2018 Factu CR - Todos los derechos reservados
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <script>
        $(document).ready(function () {
            $('.dropdown-submenu a.test').on("click", function (e) {
                $(this).next('ul').toggle();
                e.stopPropagation();
                e.preventDefault();
            });
        });
    </script>
